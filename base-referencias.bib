	author = {Soares, Vit{\'{o}}ria R. B. and Rocha, F{\'{a}}bio R. P. and Reis, Boaventura F. and Teixeira, Leonardo S. G.},
	doi = {http://dx.doi.org/10.5935/0100-4042.20150090},
