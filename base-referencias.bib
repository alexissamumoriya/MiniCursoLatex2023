@article{Sartori2009,
	abstract = {O objetivo deste trabalho foi realizar uma avalia{\c{c}}{\~{a}}o t{\'{e}}cnico-econ{\^{o}}mica da implanta{\c{c}}{\~{a}}o de unidades de extra{\c{c}}{\~{a}}o de {\'{o}}leos vegetais, buscando a inser{\c{c}}{\~{a}}o da agricultura familiar na cadeia de produ{\c{c}}{\~{a}}o do biodiesel. Foram propostos tr{\^{e}}s modelos de projetos (arranjos) de unidades de extra{\c{c}}{\~{a}}o de {\'{o}}leos vegetais em diferentes tamanhos, utilizando extra{\c{c}}{\~{a}}o por prensagem e extra{\c{c}}{\~{a}}o mista (extra{\c{c}}{\~{a}}o por prensagem e por solvente) e diferentes oleaginosas (mamona e soja). Para a an{\'{a}}lise de escala, utilizaram-se escalas de produ{\c{c}}{\~{a}}o de {\'{o}}leo entre 50 e 60 toneladas por dia, com dados reais de mercado quanto a investimentos, pre{\c{c}}os de insumos de processo, oleaginosas e {\'{o}}leo vegetal. Quanto {\`{a}}s an{\'{a}}lises econ{\^{o}}micas, utilizou-se o software BioSoft, aplicativo de apoio {\`{a}} decis{\~{a}}o, desenvolvido para an{\'{a}}lise de unidades de produ{\c{c}}{\~{a}}o de biodiesel, adaptado {\`{a}} avalia{\c{c}}{\~{a}}o de unidades de extra{\c{c}}{\~{a}}o de {\'{o}}leo vegetal. Como resultado, constatou-se que a unidade de extra{\c{c}}{\~{a}}o qu{\'{i}}mica que produz {\'{o}}leos de mamona e soja apresenta resultados mais satisfat{\'{o}}rios. No entanto, este modelo privilegia um menor retorno social, pois o n{\'{u}}mero de fam{\'{i}}lias envolvidas {\'{e}} menor, visto que, ao usar a soja, ele trabalha com apenas 50{\%} da mat{\'{e}}ria-prima proveniente da agricultura familiar.This study aimed at the technical-economical evaluation of implanting some vegetal-oil extraction units in order to insert the family agriculture into bio-diesel production chain. Three project models (arrangements) for vegetal-oil extraction units at different sizes were proposed, using extraction by pressing and mixed extraction (extracting by either pressing or solvent) and different raw materials (castor-oil and soybean). For the size analysis, the oil production scales between 50/60 tons/day were used with real market data concerning investments, inputs prices of the process, raw materials and vegetal oil. For the economic analyses, the software BioSoft was used. The BioSoft is a program for supporting the decision-making process, which was developed through analyzing units of bio-diesel production, and it was adapted to evaluate units of vegetal oil extraction. It was concluded that the unit of chemical extraction producing castor and soybean oils shows more satisfactory results. However, this model presents less social return because the number of the families involved is lower, once, in using soybean, it operates with only 50{\%} of raw material deriving from family agriculture.},
	author = {Sartori, Marco Ant{\^{o}}nio and Perez, Ronaldo and {da Silva J{\'{u}}nior}, Aziz Galv{\~{a}}o and Machado, S{\'{i}}lvia Regina Sartori and Santos, Manoela Maciel de Souza and Miranda, Carlos Alberto de Castro},
	doi = {10.1590/s0103-20032009000200005},
	journal = {Revista de Economia e Sociologia Rural},
	number = {2},
	pages = {419--434},
	title = {{An{\'{a}}lise de arranjos para extra{\c{c}}{\~{a}}o de {\'{o}}leos vegetais e suprimento de usina de biodiesel}},
	volume = {47},
	year = {2009}
}
@article{Sartori2009,
	abstract = {O objetivo deste trabalho foi realizar uma avalia{\c{c}}{\~{a}}o t{\'{e}}cnico-econ{\^{o}}mica da implanta{\c{c}}{\~{a}}o de unidades de extra{\c{c}}{\~{a}}o de {\'{o}}leos vegetais, buscando a inser{\c{c}}{\~{a}}o da agricultura familiar na cadeia de produ{\c{c}}{\~{a}}o do biodiesel. Foram propostos tr{\^{e}}s modelos de projetos (arranjos) de unidades de extra{\c{c}}{\~{a}}o de {\'{o}}leos vegetais em diferentes tamanhos, utilizando extra{\c{c}}{\~{a}}o por prensagem e extra{\c{c}}{\~{a}}o mista (extra{\c{c}}{\~{a}}o por prensagem e por solvente) e diferentes oleaginosas (mamona e soja). Para a an{\'{a}}lise de escala, utilizaram-se escalas de produ{\c{c}}{\~{a}}o de {\'{o}}leo entre 50 e 60 toneladas por dia, com dados reais de mercado quanto a investimentos, pre{\c{c}}os de insumos de processo, oleaginosas e {\'{o}}leo vegetal. Quanto {\`{a}}s an{\'{a}}lises econ{\^{o}}micas, utilizou-se o software BioSoft, aplicativo de apoio {\`{a}} decis{\~{a}}o, desenvolvido para an{\'{a}}lise de unidades de produ{\c{c}}{\~{a}}o de biodiesel, adaptado {\`{a}} avalia{\c{c}}{\~{a}}o de unidades de extra{\c{c}}{\~{a}}o de {\'{o}}leo vegetal. Como resultado, constatou-se que a unidade de extra{\c{c}}{\~{a}}o qu{\'{i}}mica que produz {\'{o}}leos de mamona e soja apresenta resultados mais satisfat{\'{o}}rios. No entanto, este modelo privilegia um menor retorno social, pois o n{\'{u}}mero de fam{\'{i}}lias envolvidas {\'{e}} menor, visto que, ao usar a soja, ele trabalha com apenas 50{\%} da mat{\'{e}}ria-prima proveniente da agricultura familiar.This study aimed at the technical-economical evaluation of implanting some vegetal-oil extraction units in order to insert the family agriculture into bio-diesel production chain. Three project models (arrangements) for vegetal-oil extraction units at different sizes were proposed, using extraction by pressing and mixed extraction (extracting by either pressing or solvent) and different raw materials (castor-oil and soybean). For the size analysis, the oil production scales between 50/60 tons/day were used with real market data concerning investments, inputs prices of the process, raw materials and vegetal oil. For the economic analyses, the software BioSoft was used. The BioSoft is a program for supporting the decision-making process, which was developed through analyzing units of bio-diesel production, and it was adapted to evaluate units of vegetal oil extraction. It was concluded that the unit of chemical extraction producing castor and soybean oils shows more satisfactory results. However, this model presents less social return because the number of the families involved is lower, once, in using soybean, it operates with only 50{\%} of raw material deriving from family agriculture.},
	author = {Sartori, Marco Ant{\^{o}}nio and Perez, Ronaldo and da {Silva J{\'{u}}nior}, Aziz Galv{\~{a}}o and Machado, S{\'{i}}lvia Regina Sartori and Santos, Manoela Maciel de Souza and Miranda, Carlos Alberto de Castro},
