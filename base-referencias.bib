@article{Biget2015,
	abstract = {{\textcopyright} 2014 Canadian Society for Chemical Engineering. The extended UNIQUAC model has been used for the representation of the thermodynamic behaviour of CO 2  absorption in aqueous amine solutions. Based on available experimental data, an identification methodology has been developed to fit the extended-UNIQUAC model parameters. In the scope of providing a robust methodology, a combination of two successive optimization methods has been chosen: a genetic algorithm and a quasi-Newton method. The first quasi-global method allows to screen the entire search space without a precise initialization, and provides an approximate solution which is then refined by the second local method. The developed multi-step regression strategy has been successfully applied to the H 2 O- monoethanolamine(MEA)-CO 2  system. The model gives a good agreement with the experimental vapour-liquid equilibria for CO 2  partial pressures and total pressures for all MEA concentrations and for a wide range of temperature with an average absolute relative deviation of around 20{\%}. Furthermore, the model predicts accurately literature data on excess enthalpy and bubble point of the system. This identification procedure has been successfully extended on several ternary H 2 O-amine-CO 2  solvent systems such as methyldiethanolamine (MDEA) and 2-amino-2-methyl-1-propanol (AMP). The wide variety of operating configurations and solvent types used and presented in this work proves the robustness and the efficiency of the developed identification method.},
	author = {Biget, Anthony and Neveux, Thibaut and Corriou, Jean Pierre and {Le Moullec}, Yann},
	doi = {10.1002/cjce.22109},
	file = {:home/moriya/Documentos/Mestrado em Sustentabilidade/DEFESA/Papers/Development of a Thermodynamic Identification Tool for CO 2
	Capture by Chemical Absorption.pdf:pdf},
	issn = {1939019X},
	journal = {Canadian Journal of Chemical Engineering},
	keywords = {CO2 solubility,Extended-UNIQUAC,Identification,Liquid-vapour equilibrium,Optimization},
	number = {2},
	pages = {297--303},
	title = {{Development of a thermodynamic identification tool for CO2 capture by chemical absorption}},
	volume = {93},
	year = {2015}
}
