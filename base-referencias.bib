@article{Rocha2009a,
	abstract = {An approach is presented to calculate solid-liquid phase equilibrium for binary mixtures, using expressions for the temperature as a function of the molar fraction. For Margules model the expression gives explicitly the temperature, while for other liquid phase activity models an iterative procedure is required to calculate the temperature. The method is very easy to apply and it can be used for mixtures that have peritectic and eutectic points, or just a eutectic point. The approach was applied to five case studies with binary mixtures of fatty acids and triglycerides. The results were in good agreement with experimental data. {\textcopyright} 2009 Elsevier B.V. All rights reserved.},
	author = {Rocha, Stella A. and Guirardello, Reginaldo},
	doi = {10.1016/j.fluid.2009.03.020},
	file = {:home/moriya/Documentos/Mestrado em Sustentabilidade/DEFESA/Papers/An approach to calculate solid–liquid phase equilibrium:},
	issn = {03783812},
	journal = {Fluid Phase Equilibria},
	keywords = {Convexity analysis,Eutectic point,Gibbs free energy minimization,Kuhn-Tucker conditions,Margules model,Peritectic point,Solid-liquid equilibrium},
	number = {1},
	pages = {12--21},
	title = {{An approach to calculate solid-liquid phase equilibrium for binary mixtures}},
	volume = {281},
	year = {2009}
}

